
public class DecimalToBinary {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		public void printBinaryFormat(int num)
		{
			int binary[] = new int[25];
			int index = 0;
			while(num>0)
			{
				binary[index++]=num%2;
				num = num/2;
			}
			
			for(i=index-1;i>=0;i--)
			{
				System.out.println(binary[i]);
			}
			
		}

	}

}
